<Page
    x:Class="WpfPaging.Pages.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WpfPaging.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewmodels="clr-namespace:WpfPaging.ViewModels"
    Title="Page1"
    d:DesignHeight="450"
    d:DesignWidth="800"
    DataContext="{Binding MainPageViewModel, Source={StaticResource ViewModelLocator}}"
    FontSize="50"
    mc:Ignorable="d">


    <Grid>
        <StackPanel>
            <Button Command="{Binding GoToBanks}">Банки</Button>
            <Button Command="{Binding GoToPlans}">Предложения</Button>
        </StackPanel>
    </Grid>
</Page>
